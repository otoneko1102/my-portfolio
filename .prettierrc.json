{
  "tabWidth": 2,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-svelte"
  ],
  "overrides": [
    {
      "files": [
        "*.json"
      ],
      "options": {
        "printWidth": 40
      }
    },
    {
      "files": [
        "src/settings/data/**"
      ],
      "options": {
        "printWidth": 200
      }
    }
  ]
}
